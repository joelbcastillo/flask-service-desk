{
  "name": "servicedesk",
  "version": "1.0.0",
  "description": "A Flask application to manage service requests using Github Issues.",
  "scripts": {
    "build": "set NODE_ENV=production && webpack --progress --colors -p",
    "build-windows": "set NODE_ENV=production && webpack --progress --colors -p",
    "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "webpack-dev-server": "set NODE_ENV=debug && webpack-dev-server --port 2992 --hot --inline",
    "webpack-dev-server-linux": "set NODE_ENV=debug && webpack-dev-server --port 2992 --hot --inline",
    "flask-server": "pipenv run flask run",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelbcastillo/servicedesk.git"
  },
  "author": "Joel Castillo",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/joelbcastillo/servicedesk/issues"
  },
  "homepage": "https://github.com/joelbcastillo/servicedesk#readme",
  "dependencies": {
    "bootstrap": "*",
    "cross-env": "*",
    "font-awesome": "*",
    "jquery": "*",
    "popper*js": "*"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-preset-env": "*",
    "concurrently": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "font-awesome-webpack": "*-beta*",
    "less": "*",
    "less-loader": "*",
    "manifest-revision-webpack-plugin": "*",
    "raw-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*",
    "sync-exec": "*"
  }
}
